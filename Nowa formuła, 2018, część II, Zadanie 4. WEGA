#include <iostream>
#include <fstream>
using namespace std;
int main()
{
    string sygnal;
    ifstream wczytaj("przyklad.txt");
    ofstream wypisz("wyniki4.txt");
    wypisz<<"Zadanie 4.1."<<endl;
    for(int i=1; i<=1000; i++)
    {
        wczytaj>>sygnal;
        if(i%40==0)
            wypisz<<sygnal[9];
    }
    return 0;
}
