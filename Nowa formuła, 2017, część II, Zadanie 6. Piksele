#include <iostream>
#include <fstream>
using namespace std;
int main()
{
    int piksel, najjasniejszy=0, najciemniejszy=225;
    ifstream wczytaj("przyklad.txt");
    ofstream wypisz("wyniki6.txt");
    for(int i=0; i<200; i++)
    {
        for(int j=0; j<320; j++)
        {
            wczytaj>>piksel;
            if(piksel>najjasniejszy)
                najjasniejszy=piksel;
            if(piksel<najciemniejszy)
                najciemniejszy=piksel;
        }
    }
    wypisz<<"Zadanie 6.1."<<endl<<najjasniejszy<<" (najjasniejszy) i "<<najciemniejszy<<" (najciemniejszy)";
    return 0;
}
